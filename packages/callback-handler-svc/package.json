{
  "name": "@mojaloop/callback-handler-svc",
  "version": "0.0.1",
  "description": "",
  "author": "Kevin Leyow",
  "contributors": [
    "Kevin Leyow <kevin.leyow@infitx.com>"
  ],
  "repository": "https://github.com/mojaloop/callback-handler-svc",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": "=18.x"
  },
  "scripts": {
    "start:ts": "ts-node -r tsconfig-paths/register -P ./tsconfig.json src/index.ts",
    "start": "node ./dist/index.js",
    "dev": "nodemon --watch './src/**/*.ts' --exec 'ts-node' src/index.ts",
    "build": "tsc -p ./tsconfig.json",
    "lint": "eslint ./src/**/*.ts *.js",
    "lint:fix": "eslint --fix ./src/**/*.ts *.js",
    "clean": "rm -rf dist",
    "test": "npm run test:unit",
    "test:coverage": "jest --coverage --coverageThreshold='{}' --testMatch '**/test/unit/**/*.(test|spec).ts'",
    "test:coverage-check": "jest -w 1 --coverage --testMatch '**/test/unit/**/*.(test|spec).ts'",
    "test:junit": "jest --reporters=default --reporters=jest-junit",
    "test:unit": "jest -w 1 --testMatch '**/test/unit/**/*.(test|spec).ts'",
    "audit:resolve": "SHELL=sh resolve-audit --production",
    "audit:check": "SHELL=sh check-audit --production"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "@callback-handler-svc/utils": "file:./dist/local-packages/utils",
    "@mojaloop/central-services-logger": "11.2.0",
    "@mojaloop/central-services-metrics": "12.0.5",
    "axios": "1.5.0",
    "convict": "^6.2.4",
    "env-var": "^7.3.1",
