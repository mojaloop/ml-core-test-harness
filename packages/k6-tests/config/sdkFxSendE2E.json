{
  "scenarios": {
    "sdkFxSendE2E": {
      "executor": "constant-vus",
      "exec": "sdkFxSendE2EScenarios",
      "env": {
        "UNIDIRECTIONAL": "true"
      },
      "vus": 10,
      "duration": "5m"
    }
  },
  "thresholds": {
    "iteration_duration": [ "p(95)<8000" ],
    "http_req_failed": [ "rate<0.01" ],
    "http_req_duration": [ "p(95)<5000" ]
  }
}
