{
  "scenarios": {
    "sdkSendE2E": {
      "executor": "constant-vus",
      "exec": "sdkSendE2EScenarios",
      "env": {
        "UNIDIRECTIONAL": "true"
      },
      "vus": 1,
      "duration": "5m"
    }
  },
  "thresholds": {
    "iteration_duration": [ "p(95)<1000" ],
    "http_req_failed": [ "rate<0.01" ],
    "http_req_duration": [ "p(95)<1000" ]
  }
}
