{
    "scenarios": {
      "outboundSDKTransfers": {
        "executor": "ramping-vus",
        "exec": "outboundSDKTransfersScenarios",
        "env": {
          "UNIDIRECTIONAL": "true"
        },
        "startVUs": 1,
        "stages": [
          { "duration": "2m", "target": 20 }
        ]
      }
    },
    "thresholds": {
      "iteration_duration": [ "p(95)<1000" ],
      "http_req_failed": [ "rate<0.01" ],
      "http_req_duration": [ "p(95)<1000" ]
    }
  }
